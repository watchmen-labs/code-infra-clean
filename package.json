{
  "name": "next-flask",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "flask-dev": "pip3 install -r requirements.txt && python -m flask --app api/index run -p 5328",
    "next-dev": "next dev",
    "dev": "concurrently \"pnpm run next-dev\" \"pnpm run flask-dev\"",
    "build": "pip3 install -r requirements.txt && next build",
    "flask-prod": "python3 -m flask --app api/index run -p 5328",
    "start": "concurrently \"next start\" \"pnpm run flask-prod\"",
    "lint": "next lint"
  },
  "dependencies": {
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@types/react-dom": "18.2.4",
    "autoprefixer": "10.4.14",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "concurrently": "^8.0.1",
    "eslint": "8.40.0",
    "eslint-config-next": "13.4.2",
    "lucide-react": "^0.537.0",
    "next": "^15.4.6",
    "next-themes": "^0.4.6",
    "ocaml-wasm": "^4.12.0-0",
    "postcss": "^8.5.6",
    "prismjs": "^1.30.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-markdown": "^10.1.0",
    "react-simple-code-editor": "^0.14.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "3.3.2",
    "typescript": "5.0.4",
    "vitest": "^3.2.4",
    "wasi-kernel": "^0.1.6"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.16",
    "@types/jest": "^30.0.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.1.4",
    "@types/prismjs": "^1.26.5",
    "@types/react": "18.2.6",
    "jest": "^30.0.5",
    "pyodide": "^0.28.2",
    "ts-jest": "^29.4.1"
  }
